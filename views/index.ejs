<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Shop</title>
  </head>
  <body>
    <h1>GROCERY SHOP</h1>
    <h2><%= title %></h2>
    <div>
      <% items.forEach(item => { %>
          <div>
            <span><%= item.name %></span>
            <span><%= item.category %></span>
            <img src="<%= item.imageurl %>" alt="">
            <span><%= item.price %></span>
          </div>
      <% }) %>
    </div>
  </body>
</html>
